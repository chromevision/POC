{
	"name": "ChromeVision",
	"version": "1.0",
	"description":
		"Capture your emotions as you browse the web and visualize aggregated data!",
	"permissions": [
		"activeTab",
		"declarativeContent",
		"storage",
		"tabs",
		"https://eastus.api.cognitive.microsoft.com/face/v1.0/detect",
		"http://localhost:3000/api/webcam",
		"<all_urls>"
	],
	"options_page": "options.html",
	"icons": {
		"16": "images/cvl16.png",
		"32": "images/cvl32.png",
		"48": "images/cvl48.png",
		"128": "images/cvl128.png"
	},
	"manifest_version": 2
}
